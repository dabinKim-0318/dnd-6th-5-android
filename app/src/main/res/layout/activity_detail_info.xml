                        android:text="@string/detail_info_policy_title"
                        android:textColor="@color/gray06"
                        app:layout_constraintBottom_toBottomOf="@id/tv_detail_info_policy_name"
                        app:layout_constraintStart_toEndOf="@id/tv_detail_info_policy_name"
